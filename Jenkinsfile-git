pipeline {
  agent { dockerfile true }
  stages {
    stage('Test') {
      steps {
        withCredentials([gitUsernamePassword(credentialsId: 'git_cred', gitToolName: 'git-tool')]) {
          sh 'git clone https://github.com/shadelurk/mdouttest.git'
          sh 'rm -r mdouttest'
          sh 'python pull_and_rmdir2.py'
        }
      }
    }
  }
}