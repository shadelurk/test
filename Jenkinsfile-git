pipeline {
  agent { dockerfile true }
  environment {
      MY_TOKEN = credentials('TOKEN')
  }
  stages {
    stage('Test') {
      steps {
        withCredentials([usernamePassword(credentialsId: '055build', usernameVariable: 'GIT_USERNAME', passwordVariable: 'GIT_PASSWORD')]) {
          sh 'python pull_and_rmdir2.py'
          sh 'python sheet_work_download2.py ${MY_TOKEN}'
          sh "export GIT_CREDENTIALS=$GIT_USERNAME:$GIT_PASSWORD && python push_sheet_work2.py"
        }
      }
    }
  }
}